// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model File {
  id  Int @default(autoincrement()) @id
  path String @unique
  userEmail String
  revision Int
  commitId String
  hash String
  size Int
  s3Link String
}